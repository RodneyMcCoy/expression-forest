INCLUDES= -I./lib/Dictionary -I./lib/ExpressionTree -I./lib/Parser -I./tests
CFLAGS= -Wall



# ----- MAKEFILE FOR TESTS ------
arrayTest:
	g++ $(CFLAGS) $(INCLUDES) tests/arrayTest.cpp -o arrayTest
	chmod +x arrayTest
	./arrayTest

strTest:
	g++ $(CFLAGS) $(INCLUDES) tests/strTest.cpp ./lib/Dictionary/str.cpp -o strTest
	chmod +x strTest
	./strTest

symTest:
	g++ $(CFLAGS) $(INCLUDES) tests/symTest.cpp ./lib/Dictionary/sym_table.cpp ./lib/Dictionary/str.cpp -o symTest
	chmod +x symTest
	./symTest

dictTest:
	g++ $(CFLAGS) $(INCLUDES) tests/dictTest.cpp -o dictTest
	chmod +x dictTest
	./dictTest



build:
	echo "Build Here"



clean:
	$(RM) arrayTest
	$(RM) strTest
	$(RM) symTest
	$(RM) dictTest
